= content_for :head do
  = render partial: 'shared/setup_uploadify'
  :javascript
    I18n = #{ translations_to_json(['contests', 'space']) }
    $(document).ready(function(){
      SpacePicturesUploader.init(I18n.photos)
    })

- content_for :client_center_content do
  = form_tag contest_path(id: @contest.id, pictures_dimension: true), { id: 'design_space', method: 'put', } do
    .rooms.text-package.text-center-sm.text-center-xs.rooms-conditional-options
      .space-view-details
        = render partial: 'contests/options/space_pictures_options'
        = render partial: 'contests/options/space_dimensions_options'
    .text-center.marg-top30
      %input.btn1.btn1-confirm{:name => "commit", :type => "submit", :value => "Save"}
= render partial: 'clients/client_center/layout'

