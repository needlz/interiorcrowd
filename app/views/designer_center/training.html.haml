- content_for :designer_center_content do
  .container.animated.fadeInDown.bottom60.lightFont
    .row
      .col-md-10.col-md-offset-1
        %h3= t('.check_out')
        - t('.blog_articles.').each do |article|
          = link_to article[:title],
            URI.decode( designers_blog_page_path( blog_page_path: article[:path] ) ),
            class: "designerNotification latestTraining #{ tracked_link }"

= render partial: 'designer_center/layout'
