.space-options
  .dimensions
    .col-md-12.no-padding-left
      %h1.title= t('contests.space.dimensions.title')
      %span.hint= t('contests.space.dimensions.hint', faqs_link: link_to(t('contests.space.dimensions.faqs'), faq_path)).html_safe
    .clearfix.dimensions-margin
    = render partial: 'contests/options/dimension_of_space_row',
             collection: @contest_view.dimensions,
             as: :dimension
    .clearfix
