- value_html = capture do
  - @contest_view.space_pictures.try(:each) do |image|
    = render partial: 'shared/image_with_enlarge_button',
        locals: { container_class: 'pull-left',
                  image_class: 'fullWidth',
                  thumb_path: image.medium_size_url,
                  original_size_path: image.original_size_url,
                  enlarge_button_class: 'space' }
= render partial: 'contests/previews/preview_row', locals: { title: t('contests.titles.brief.space_pictures'), value: value_html }
