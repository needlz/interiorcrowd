#finishContest.modal.fade{ area: { hidden: 'true', labelledby: 'myModalLabel' }, role: 'dialog', tabindex: '-1' }
  .modal-dialog
    .modal-content
      .modal-body
        %p= t('designer_center.final_upload.ready_to_send').html_safe
      .modal-footer.disablePadding
        .row.disableMargin
          %a.col-md-6.yes-button{:href => "#"}= t('designer_center.final_upload.yes')
          %a.col-md-6.no-button{:href => "#"}= t('designer_center.final_upload.no')
