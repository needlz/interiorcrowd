= content_for :head do
  = javascript_include_tag 'sessions/login'

- login_box_footer = capture do
  #signInHint
    %p
      = t('login.designer.dont_have_account')
      %a{ href: designer_submission_path, class: tracked_link }= t('login.designer.sign_up')
  = render 'sessions/signin_footer'

= render partial: 'sessions/login_box',
    locals: { footer: login_box_footer, login_view: @login_view }
